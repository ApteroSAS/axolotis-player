{
  "name": "@aptero/axolotis-player",
  "version": "2.1.3",
  "description": "Axolotis - player",
  "repository": {
    "type": "git",
    "url": "https://github.com/ApteroSAS/axolotis-player"
  },
  "bugs": {
    "url": "https://github.com/ApteroSAS/axolotis-player/issues"
  },
  "author": "Aptero <admin@aptero.co> (https://github.com/ApteroSAS/axolotis-player)",
  "license": "MIT",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "files": [
    "*"
  ],
  "scripts": {
    "build": "webpack && tsc && ttsc --emitDeclarationOnly",
    "start": "webpack serve --config webpack.config.demo.js",
    "build:dev": "cross-env DEVTOOL=eval-source-map yarn run build",
    "build:demo": "webpack --config webpack.config.demo.js",
    "dev-watch": "tsc-watch --onSuccess \"yarn run build\"",
    "test": "jest --silent",
    "coverage": "jest --silent --ci --runInBand --reporters=default --reporters=jest-junit --coverage --collectCoverageFrom='src/lib/**/*.ts'",
    "trypublish": "npm publish || true"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.5",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-transform-typescript": "^7.18.4",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.18.2",
    "@types/jest": "^28.1.1",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.5",
    "babel-preset-minify": "^0.5.2",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.17.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "jest-junit": "^13.2.0",
    "mini-css-extract-plugin": "^2.6.0",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "prettier": "^2.7.0",
    "prettier-webpack-plugin": "^1.2.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.3",
    "tsc-watch": "^5.0.3",
    "tsutils": "^3.21.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.7.3",
    "typescript-transform-paths": "^3.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "4.9.2"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "@root(.*)$": "<rootDir>/src/$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/scripts/testMock.js",
      "\\.(css|less)$": "<rootDir>/scripts/testMock.js"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ]
}
